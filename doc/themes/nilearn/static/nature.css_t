    max-width:805px;
}

/* ------- alternating colors in table rows -------------------------- */
table.docutils {
    border: 0;
    border-collapse: collapse;
}

table.docutils tr:nth-child(even) {
    background-color: #F3F3FF;
}
table.docutils tr:nth-child(odd) {
    background-color: #FFFFEE;
}

table.docutils tr {
    border-style: solid;
    border-width: 1px 0 1px 0;
    border-color: #AAAAAA;
}

table.docutils td {
    border-color: #ddd;
}

/* ------- formatting of the docstrings ------------------- */

/* Space between parameter name and description */
dl.field-list span.classifier {
    margin-left: 1ex;
}

dl.function td.field-body ul.first, dl.class td.field-body ul.first {
    margin: 3ex 0 3ex -6ex;
    list-style: none;
    text-indent: -4ex;
}

dl.function td.field-body ul.first > li, dl.class td.field-body ul.first > li {
    margin-bottom: 1.5ex;
}

dl.function tr:nth-child(2n+1), dl.class tr:nth-child(2n+1) {
    background-color: #FFF;
    border-width: 0;
}

dl.function tr:nth-child(2n), dl.class tr:nth-child(2n) {
    background-color: #F6FDFF;
    border-width: 0;
}

dl.function table.docutils td, dl.class table.docutils td {
    border-width: 1px 0 1px 0;
}

dl.function .rubric, dl.class .rubric {
    margin-top: 4ex;
    border: solid #AAA;
    border-width: 0 0 1px 0;
}

/* Matching our dostrings to the very recent numpydoc evolutions */
dl.function dt.field-odd, dl.class dt.field-odd {
    font-weight: bold;
}

dl.class dt.field-odd {
    font-weight: bold;
}
dl.class dl.method dl.field-list dt.field-even {
    font-weight: bold;
}
dl.class dl.method dl.field-list dl.simple dt {
    font-weight: bold;
}
dl.class dl.method dl.field-list dl.simple dt span.classifier {
    font-weight: normal;
}

dl.method {
    margin-top: 6ex;
}

dl.attribute {
    margin-top: 2ex;
}

dl.method code, dl.attribute code {
    font-weight: 600;
}


/* ------- tighter layout in pre -------------------------- */
pre {
