/* Sphinx-gallery Report embedding */
div.sg-report {
    padding: 0pt;
    transform: scale(.95);
}

div.body iframe {
    width: 100%;
}

div.sg-report iframe {
    display: block;
    border-style: none;
    transform: scale(.85);
    height: 470px;
    margin-left: -12%; /* Negative because of .8 scaling */
    margin-top: -4%;
    padding: 0pt;
    margin-bottom: 0pt;
    width: 126%; /* More than 100% because of .8 scaling */
}

/* gallery download note */

div.body div.sphx-glr-download-link-note {
    float: left;
    text-align: left;
    border-radius: 4px;
    color: #006801;
    background-color: #dff0d8;
    border: 0.2em solid rgba(79, 173, 117, 0.35);
    padding: 4px;
    max-width: 100%;
    width: 43ex;
}

@media (min-width: 1200px) {
  div.body div.sphx-glr-download-link-note {
    /* Flush to the left: 50% of viewport width
     * minus half of the bodywrapper width,
     * minus the half width of the sphinxsidebar */
    float: none;
    margin-left: calc(32em - 50vw + 123px);
    margin-bottom: -10ex;
}

@media (min-width: 1420px) {
  div.body div.sphx-glr-download-link-note {
    position: absolute;
    left: 5px;
    width: 15ex;
    margin-left: 0px;
  }
}

@media (min-width: 1550px) {
    div.body div.sphx-glr-download-link-note {
	width: 20ex;
	/* 47% of viewport width minus half of the bodywrapper width,
	 * minus the half width of the sphinxsidebar, minus the width of this
	 * div */
	margin-left: calc(47vw - 31em - 123px - 20ex);
    }
}
